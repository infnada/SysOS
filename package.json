  "bugs": "https://github.com/anyOpsOS/anyOpsOS/issues",
  "keywords": [],
  "description": "Web based operating system for SysOps, DevOps, NetOps, SecOps... Monitor, manage, configure, backup & recovery your virtual and physical infrastructure.",
  "author": "Isart Navarro <contact@isartnavarro.io> (https://isartnavarro.io)",
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "private": false,
  "scripts": {
    "prod:start": "node dist/anyOpsOS/index.js",
    "back:dev:start": "node --stack-size=1024 --stack-trace-limit=1000 dist/anyOpsOS/index.js",
    "front:dev:start": "npm-run-all build dev:proxy",
    "dev:proxy": "ng serve --proxy-config proxy.conf.json",
    "copy-apps": "npm-run-all copy-umds move-umd-deps",
    "build": "npm-run-all clean lint-backend lint-frontend build-backend build-libs build-apps build-modals build-frontend copy-extra copy-apps",
    "build-libs": "node scripts/build-libs.js",
    "build-apps": "node scripts/build-apps.js",
    "build-modals": "node scripts/build-modals.js",
    "build-frontend": "ng build",
    "build-backend": "tsc --build src/backend/tsconfig.json",
    "lint-frontend": "ng lint",
    "lint-backend": "tslint -p src/backend/tsconfig.json",
    "ng": "ng",
    "test": "ng test",
    "e2e": "ng e2e",
    "clean": "rm -rf dist/anyOpsOS/*",
    "copy-extra": "cp -rf src/backend/app/ssl dist/anyOpsOS/ && cp -rf src/backend/app/filesystem dist/anyOpsOS/ && cp -rf node_modules/mxgraph/javascript/src/css dist/anyOpsOS/filesystem/etc/applications/drawer && cp -rf node_modules/draw.io/drawio/src/main/webapp/resources dist/anyOpsOS/filesystem/etc/applications/drawer && cp -rf node_modules/draw.io/drawio/src/main/webapp/stencils/* dist/anyOpsOS/filesystem/etc/applications/drawer/stencils/ && cp -rf node_modules/draw.io/drawio/src/main/webapp/templates dist/anyOpsOS/filesystem/etc/applications/drawer && cp -rf node_modules/draw.io/drawio/src/main/webapp/images dist/anyOpsOS/filesystem/etc/applications/drawer && cp -rf node_modules/draw.io/drawio/src/main/webapp/shapes dist/anyOpsOS/filesystem/etc/applications/drawer",
    "copy-umds": "cp dist/applications/*/bundles/*.umd.js dist/anyOpsOS/filesystem/bin/applications/ && cp dist/modals/*/bundles/*.umd.js dist/anyOpsOS/filesystem/bin/modals/ && cp dist/libs/anyopsos-lib-ext-*/bundles/*.umd.js dist/anyOpsOS/filesystem/bin/libs/ && npm run postscript",
    "move-umd-deps": "mv dist/anyOpsOS/filesystem/bin/libs/anyopsos-lib-ext-netdata.umd.js dist/anyOpsOS/filesystem/bin/libs/deps/anyopsos-lib-ext-netdata.umd.js",
    "postscript": "node scripts/postscript.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
